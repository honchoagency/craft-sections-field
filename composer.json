{
    "name": "honchoagency/craft-sections-field",
    "description": "Allows users to select from a list of sections on the site with a simple multi-select field.",
    "type": "craft-plugin",
    "license": "mit",
    "version": "1.0.1",
    "minimum-stability": "dev",
    "authors": [
        {
            "name": "honchoagency",
            "homepage": "https://honcho.agency"
        }
    ],
    "support": {
        "email": "dev@honcho.agency",
        "issues": "https://github.com/honchoagency/craft-sections-field/issues?state=open",
        "source": "https://github.com/honchoagency/craft-sections-field",
        "docs": "https://github.com/honchoagency/craft-sections-field",
        "rss": "https://github.com/honchoagency/craft-sections-field/releases.atom"
    },
    "require": {
        "php": ">=8.2",
        "craftcms/cms": "^5.0"
    },
    "require-dev": {
        "craftcms/ecs": "dev-main",
        "craftcms/phpstan": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "honcho\\sectionsfield\\": "src/"
        }
    },
    "extra": {
        "handle": "sections-field",
        "name": "Sections Field",
        "developer": "Honcho Agency",
        "documentationUrl": "https://github.com/honchoagency/craft-sections-field",
        "class": "honcho\\sectionsfield\\SectionsFieldPlugin"
    },
    "scripts": {
        "check-cs": "ecs check --ansi",
        "fix-cs": "ecs check --ansi --fix",
        "phpstan": "phpstan --memory-limit=1G"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}
